{
  "name": "next-ts-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Next starter template with Typescript, TailwindCSS, Styled Components and eslint config",
  "keywords": [
    "next",
    "next-boot",
    "next-template",
    "tailwindcss",
    "styled-components",
    "next-typescript",
    "eslint",
    "boot"
  ],
  "author": "Carlos Ernesto Brooks Lorie (https://github.com/carlosblinf)",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "tailwindcss": "^3.2.4",
    "twin.macro": "^3.1.0",
    "typescript": "4.9.4"
  }
}
